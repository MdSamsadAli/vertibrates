@model vertibrates.Models.project_table
<div class="form-horizontal">
	<div class="row form-group">
		<div class="col-lg-4 col-sm-4 col-xs-4 col-md-4">
			@Html.LabelFor(m => m.Name, new { htmlAttributes = new { @class = "" } })
			@Html.EditorFor(m => m.Name, new { htmlAttributes = new { @class = "form-control", @placeholder = "Name" } })
		</div>
		<div class="col-md-4">
			@Html.LabelFor(model => model.Address, new { htmlAttributes = new { @class = "col-md-2" } })
			@Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "form-control", @placeholder = "Address" } })
		</div>
	</div>

	<div class="row form-group">
		<div class="col-md-4">
			@Html.LabelFor(model => model.ID,"project_table2", new { htmlAttributes = new { @class = "col-md-2" } })
			@Html.DropDownList("ProjectList", null, "Select", htmlAttributes: new { @class = "form-control", @Name = "ID" })
		</div>

		<div class="col-md-4">
			@Html.LabelFor(model => model.Contact, new { htmlAttributes = new { @class = "col-md-2" } })
			@Html.EditorFor(model => model.Contact, new { htmlAttributes = new { @class = "form-control", @placeholder = "Contact" } })
		</div>
		<div class="col-md-2 pull-right">
			<input type="submit" class="btn btn-success" value="Save" />
		</div>
	</div>

</div> 